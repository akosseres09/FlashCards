{
    "firestore": {
        "database": "(default)",
        "location": "europe-central2",
        "rules": "firestore.rules",
        "indexes": "firestore.indexes.json"
    },
    "hosting": {
        "public": "dist/FlashCards/browser",
        "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    },
    "emulators": {
        "firestore": {
            "host": "127.0.0.1",
            "port": 8080
        },
        "auth": {
            "host": "127.0.0.1",
            "port": 9099
        },
        "ui": {
            "enabled": true,
            "host": "127.0.0.1",
            "port": 3000
        },
        "hosting": {
            "host": "127.0.0.1",
            "port": 5000
        }
    }
}
